	<div id="pagedescription"><h2><%= "Full history for #{@title_text}"%></h2></div>
	
	<div id="contentmiddle" class="grid_12">
	<div id="recent_history">
	<ul>
		<% unless @logs.nil? || @logs.length == 1%>
		<% @logs.each do |i| %>
			<!--<li>Hello <span>marked done on</span> Aug 5, 2011 <span>at</span> 10:24 am</li>-->
			<li><%= i.target_type %> <%= i.target_name %> <span><%= i.action %> on</span> <%= i.date %> <span>at</span> <%= i.time %> <span>by Main User</span></li>
		<% end %>
	</ul>
	<% end %>
	</div>
	</div>
</fieldset>